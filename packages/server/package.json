{
  "name": "@lemur-bookstores/server",
  "version": "0.0.1",
  "description": "Server implementation for SecureStack",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:bundle && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "build:bundle": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.9.14",
    "@grpc/proto-loader": "0.7.10",
    "@lemur-bookstores/core": "*",
    "@lemur-bookstores/auth": "*",
    "@trpc/server": "10.45.0",
    "fastify": "4.26.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "20.11.5",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "vitest": "1.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
