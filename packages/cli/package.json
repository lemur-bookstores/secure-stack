{
  "name": "@lemur-bookstores/cli",
  "version": "0.0.1",
  "description": "CLI tool for SecureStack - Project scaffolding, code generation, and development tools",
  "main": "./dist/index.js",
  "bin": {
    "securestack": "./bin/securestack.js"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "securestack",
    "cli",
    "scaffolding",
    "code-generation",
    "developer-tools"
  ],
  "author": "@elkincp5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemur-bookstores/secure-stack.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "commander": "11.1.0",
    "inquirer": "9.2.12",
    "chalk": "5.3.0",
    "ora": "8.0.1",
    "boxen": "7.1.1",
    "ejs": "3.1.9",
    "fs-extra": "11.2.0",
    "execa": "8.0.1",
    "chokidar": "3.5.3",
    "esbuild": "0.19.11",
    "zod": "3.22.4",
    "picocolors": "1.0.0"
  },
  "devDependencies": {
    "@types/node": "20.11.5",
    "@types/fs-extra": "11.0.4",
    "@types/ejs": "3.1.5",
    "tsup": "8.5.1",
    "typescript": "5.3.3",
    "vitest": "4.0.15"
  }
}
