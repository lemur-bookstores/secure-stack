{
  "name": "@lemur-bookstores/cache",
  "version": "0.1.1",
  "description": "Cache module for SecureStack with support for Redis, Memory, and Memcached",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "securestack",
    "cache",
    "redis",
    "memcached",
    "lru"
  ],
  "author": "@elkincp5",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "better-sqlite3": "12.5.0",
    "ioredis": "5.3.2",
    "lru-cache": "10.2.0",
    "memjs": "1.3.0",
    "mongodb": "7.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "7.6.13",
    "@types/memjs": "1.3.3",
    "@types/node": "^20.11.5",
    "tsup": "8.0.1",
    "typescript": "5.9.3",
    "vitest": "1.2.1"
  }
}
