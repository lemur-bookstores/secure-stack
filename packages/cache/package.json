{
  "name": "@lemur-bookstores/cache",
  "version": "0.0.1",
  "description": "Cache module for SecureStack with support for Redis, Memory, and Memcached",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "securestack",
    "cache",
    "redis",
    "memcached",
    "lru"
  ],
  "author": "@elkincp5",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "ioredis": "^5.3.2",
    "lru-cache": "^10.2.0",
    "memjs": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
