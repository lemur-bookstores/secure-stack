{
  "name": "@lemur-bookstores/client",
  "version": "0.0.1",
  "description": "SecureStack client with React hooks and tRPC integration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/vue/index.mjs",
      "require": "./dist/vue/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:bundle && npm run build:types",
    "build:bundle": "tsup",
    "build:types": "rm -f tsconfig.tsbuildinfo && tsc --emitDeclarationOnly",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "securestack",
    "client",
    "trpc",
    "react",
    "vue",
    "hooks",
    "typescript"
  ],
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "1.14.2",
    "@lemur-bookstores/core": "*",
    "@tanstack/react-query": "5.17.0",
    "@tanstack/vue-query": "5.92.0",
    "next": "14.2.33"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "vue": "3.5.25",
    "@tanstack/vue-query": "5.92.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "@tanstack/vue-query": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tanstack/vue-query": "5.92.0",
    "@testing-library/react": "14.1.2",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "happy-dom": "20.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "8.5.1",
    "typescript": "5.3.3",
    "vitest": "4.0.15",
    "vue": "3.5.25"
  }
}
