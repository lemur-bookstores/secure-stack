{
  "name": "@lemur-bookstores/client",
  "version": "0.0.1",
  "description": "SecureStack client with React hooks and tRPC integration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:bundle && npm run build:types",
    "build:bundle": "tsup",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "securestack",
    "client",
    "trpc",
    "react",
    "hooks",
    "typescript"
  ],
  "license": "MIT",
  "dependencies": {
    "@lemur-bookstores/core": "*",
    "@tanstack/react-query": "5.17.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/react": "14.1.2",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "vitest": "1.6.1"
  }
}
