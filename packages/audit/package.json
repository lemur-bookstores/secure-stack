{
  "name": "@lemur-bookstores/audit",
  "version": "0.1.1",
  "description": "Audit logging module for SecureStack",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "securestack",
    "audit",
    "logging",
    "security",
    "compliance"
  ],
  "author": "Lemur Bookstores",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.15"
  }
}
